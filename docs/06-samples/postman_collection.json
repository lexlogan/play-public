{
  "info": {
    "name": "InnerAI Hub (MVP)",
    "_postman_id": "00000000-0000-0000-0000-000000000000",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "List Projects",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer <ADMIN_JWT>"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/v1/projects",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "v1",
            "projects"
          ]
        }
      }
    },
    {
      "name": "Create Completion",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer <PROJECT_TOKEN>"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"projectId\": \"<uuid>\",\n  \"provider\": \"openai\",\n  \"model\": \"gpt-4o-mini\",\n  \"prompt\": \"Hello!\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/v1/completions",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "v1",
            "completions"
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8080"
    }
  ]
}